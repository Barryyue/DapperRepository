<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewBag.Title - Dapper.Net repository pattern demo</title>
    <link href="~/Content/layui/css/layui.css" rel="stylesheet" />
    <script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
    <script src="~/Content/layui/layui.js"></script>
</head>
<body>
    <div class="layui-main">
        <h1 style="text-align: center; color: #ffffff; background-color: #c0c0c0;">Demo based on Dapper.Net repository pattern</h1>
        <br />
        @RenderBody()
        <br />
        <footer>
            <h3 style="text-align: center;">@DateTime.Now.ToString("yyyy-MM-dd")</h3>
        </footer>
    </div>
</body>
</html>